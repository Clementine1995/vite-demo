{
  "singleQuote": true,
  "semi": false,
  "vueIndentScriptAndStyle": true,
  "jsxSingleQuote": true,
  "jsxBracketSameLine": true,
  "arrowParens": "avoid",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "printWidth": 80,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "printWidth": 200
      }
    }
  ],
  "endOfLine": "auto"
}